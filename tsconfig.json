{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": ["@types/gtag.js"],
    "paths": {
      "@inner/*": ["./src/app/(inner)/*"],
      "@api/*": ["./src/app/api/*"],
      "@/*": ["./src/*"],
      "/*": ["./*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/api/auth/logout",
    "src/app/(inner)/(pip)/setlist/_component/ext.tsxT"
  ],
  "exclude": ["node_modules", "temp", "src/model/cloudflare/index.js"]
}
